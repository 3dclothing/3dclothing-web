.expressDetail {
  position: fixed;
  width: 100%;
  height: 100%;
  background: #F7F7F7;
}

.expressDetail {
  background: #ffffff;
  color: #9B9B9B;
  font-size: 24rpx;
  padding-bottom: 40rpx;
}

.expressDetail .orderNo  {
  color: #000000;
  font-size: 28rpx;
  padding: 20rpx 40rpx;
  border-top: 1rpx solid  #D8D8D860;
  border-bottom: 1rpx solid  #D8D8D860;
}

.expressDetail .address {
  padding: 20rpx 40rpx 50rpx 40rpx;
}

.expressDetail .order-info {
  padding: 30rpx 0;
  border-top: 6rpx solid #F7F7F7;
  border-bottom: 6rpx solid #F7F7F7;
  font-size: 20rpx;
}

.expressDetail .order-info image {
  width: 76px;
  height: 76px;
  padding: 0 20rpx;
}

.expressDetail .activity {
  padding: 40rpx;
  line-height: 50rpx;
}

.expressDetail  .footer {
  position: fixed;
  width: 100%;
  bottom: 0;
}

.expressDetail  .footer .btn {
  background: #000000;
  color: #ffffff;
  font-size: 32rpx;
  line-height: 96rpx;
  margin-top: 40rpx;
  border-radius: 0;
}
